{
    "name": "vibemix",
    "description": "A powerful tool to create music compilation videos from your images and songs.",
    "author": {
        "name": "Joshua Sinaga",
        "email": "josua123690707@gmail.com",
        "url": "https://itsjo.works"
    },
    "version": "1.1.3",
    "private": false,
    "main": "dist-electron/main.js",
    "homepage": "./",
    "scripts": {
        "dev": "next dev --turbopack -p 9002",
        "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
        "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
        "build": "next build",
        "build:electron": "tsc electron/main.ts --outDir dist-electron --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports && tsc electron/preload.ts --outDir dist-electron --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports",
        "build:next-electron": "next build && tsc electron/main.ts --outDir dist-electron --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports && tsc electron/preload.ts --outDir dist-electron --target es2020 --module commonjs --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports",
        "electron": "cross-env NODE_ENV=development concurrently \"bun run dev\" \"wait-on http://localhost:9002 && electron dist-electron/main.js\"",
        "electron:dev": "bun run build:electron && bun run electron",
        "electron:build": "bun run build && bun run build:electron && electron-builder",
        "electron:build-win": "bun run build && bun run build:electron && electron-builder --win",
        "electron:build-mac": "bun run build && bun run build:electron && electron-builder --mac",
        "electron:build-linux": "bun run build && bun run build:electron && electron-builder --linux",
        "start": "next start",
        "lint": "next lint",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@ffmpeg/ffmpeg": "^0.12.15",
        "@ffmpeg/util": "^0.12.2",
        "@genkit-ai/googleai": "^1.14.1",
        "@genkit-ai/next": "^1.14.1",
        "@hookform/resolvers": "^4.1.3",
        "@radix-ui/react-accordion": "^1.2.3",
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-collapsible": "^1.1.11",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-menubar": "^1.1.6",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-progress": "^1.1.2",
        "@radix-ui/react-radio-group": "^1.2.3",
        "@radix-ui/react-scroll-area": "^1.2.3",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slider": "^1.2.3",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-tabs": "^1.1.3",
        "@radix-ui/react-toast": "^1.2.6",
        "@radix-ui/react-tooltip": "^1.1.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^3.6.0",
        "dotenv": "^16.5.0",
        "embla-carousel-react": "^8.6.0",
        "genkit": "^1.14.1",
        "lucide-react": "^0.475.0",
        "next": "15.3.3",
        "patch-package": "^8.0.0",
        "react": "^18.3.1",
        "react-day-picker": "^8.10.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.54.2",
        "recharts": "^2.15.1",
        "tailwind-merge": "^3.0.1",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@types/electron": "^1.6.12",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "concurrently": "^9.2.0",
        "cross-env": "^10.0.0",
        "electron": "^37.2.5",
        "electron-builder": "^26.0.12",
        "genkit-cli": "^1.14.1",
        "postcss": "^8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5",
        "wait-on": "^8.0.4"
    },
    "build": {
        "appId": "works.vibemix.itsjo",
        "productName": "VibeMix",
        "directories": {
            "output": "electron-dist"
        },
        "files": [
            "out/**/*",
            "dist-electron/**/*",
            "node_modules/**/*",
            "!node_modules/@electron/**/*",
            "!node_modules/electron/**/*"
        ],
        "extraResources": [
            {
                "from": "out",
                "to": "out",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "mac": {
            "icon": "public/logo_512x512.png",
            "category": "public.app-category.productivity",
            "target": [
                {
                    "target": "dmg",
                    "arch": [
                        "x64",
                        "arm64"
                    ]
                }
            ]
        },
        "win": {
            "icon": "public/logo_256x256.png",
            "target": [
                {
                    "target": "portable",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "linux": {
            "icon": "public/logo_512x512.png",
            "target": [
                {
                    "target": "AppImage",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        }
    }
}
